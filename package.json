  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "pino-pretty": "^7.6.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "axios": "^0.27.1",
